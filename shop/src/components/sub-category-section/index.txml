<view class="sub-category-section radius-8px w-full flex flex-wrap justify-around">
  <block tiki:if="{{isLoading}}">
    <category
      className="category-nav-item"
      isLoading
      tiki:for="{{skeletonsArray}}"
      tiki:key="*this"
    />
  </block>
  <block
    tiki:for="{{categories}}"
    tiki:key="id"
  >
    <category
      className="sub-category-section-item"
      category="{{item}}"
      onTapCategory="_onTapCategory"
    />
  </block>
</view>
