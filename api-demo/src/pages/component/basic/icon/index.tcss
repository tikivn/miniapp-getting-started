.container {
  min-height: 100%;
  background-color: var(--gray20);
  padding: 16px;
}

.block {
  background-color: white;
  margin: 8px 0;
  padding: 16px;
  border-radius: var(--border-radius-rounded-4px);
}

.list {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.header {
  font-size: 16px;
  font-weight: 600;
}

.item {
  display: flex;
  margin: 8px;
  justify-content: center;
  align-items: center;
}

button {
  margin-top: 8px;
  width: 100%;
}
