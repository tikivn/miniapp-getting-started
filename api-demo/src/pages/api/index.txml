<view class="page-container">
  <view class="banner" >
    <view class="search">
      <input placeholder="Tìm kiếm thành phần/API" />
    </view>
  </view>
  <view class="content">
    <block tiki:for="{{APIList}}">
      <block tiki:if="{{item.list.length > 0}}">
        <list key="list" header="{{item.type}}" items="{{item.list}}" />
      </block>
    </block>
  </view>
</view>
