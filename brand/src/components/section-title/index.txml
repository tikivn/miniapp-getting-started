<view class="flex justify-between items-center mt-4x-small mb-medium">
  <view class="font-bold text-medium">
    {{title}}
  </view>
  <view
    tiki:if="{{action}}"
    class="flex items-center"
    onTap="_onTapActionButton"
  >
    <text class="text-brand mr-2x-small">
      {{action}}
    </text>
    <icon type="arrow_right" color="#1A94FF" size="{{20}}"/>
  </view>
</view>
